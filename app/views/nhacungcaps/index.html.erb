<p id="notice"><%= notice %></p>

<h1>Nhacungcaps</h1>

<table>
  <thead>
    <tr>
      <th>Idnhacungcap</th>
      <th>Tennhacungcap</th>
      <th>Diachi</th>
      <th>Thongtindaidien</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nhacungcaps.each do |nhacungcap| %>
      <tr>
        <td><%= nhacungcap.idnhacungcap %></td>
        <td><%= nhacungcap.tennhacungcap %></td>
        <td><%= nhacungcap.diachi %></td>
        <td><%= nhacungcap.thongtindaidien %></td>
        <td><%= link_to 'Show', nhacungcap %></td>
        <td><%= link_to 'Edit', edit_nhacungcap_path(nhacungcap) %></td>
        <td><%= link_to 'Destroy', nhacungcap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

